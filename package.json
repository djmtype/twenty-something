    "@astrojs/sitemap": "^3.2.1",
    "@csstools/postcss-global-data": "^3.0.0",
    "@fontsource/cardo": "^5.1.0",
    "@iconify-json/lucide": "^1.2.13",
    "alpinejs": "3.14.3",
    "astro": "^4.16.10",
    "astro-auto-import": "^0.4.4",
    "astro-icon": "^1.1.2",
    "astro-page-insight": "^0.9.0",
    "autoprefixer": "^10.4.20",
    "github-slugger": "^2.0.0",
