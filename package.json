    "tools": "astro preferences enable devToolbar",
    "notools": "astro preferences disable devToolbar"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.14.3",
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^3.1.9",
