{
  "name": "twenty-something",
  "type": "module",
  "version": "0.6.1",
  "scripts": {
    "add": "npm install",
    "themes": "node makeThemes.mjs",
    "dev": "concurrently \"onchange './src/data/tokens.json' -- npm run themes\" \"npm run themes\" \"astro dev\"",
